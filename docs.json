{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Instalog Documentation",
  "colors": {
    "primary": "#37B550",
    "light": "#1ADA41",
    "dark": "#50945E"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "mcp",
              "best-practices"
            ]
          },
          {
            "group": "Mobile SDKs",
            "pages": [
              "sdk/index",
              "sdk/android/index",
              "sdk/ios/index",
              "sdk/react-native/index",
              "sdk/flutter/index"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "openapi": "https://raw.githubusercontent.com/Instalog-dev/open-api/refs/heads/main/openapi.json"
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Github",
          "href": "https://github.com/instalog-dev",
          "icon": "github"
        },
        {
          "anchor": "Twitter",
          "href": "https://x.com/instalog_dev",
          "icon": "twitter"
        },
        {
          "anchor": "Bluesky",
          "href": "https://bsky.app/profile/instalog.dev",
          "icon": "bluesky"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@instalog.dev"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://beta.instalog.dev/dashboard"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/instalog_dev",
      "github": "https://github.com/instalog-dev",
      "linkedin": "https://linkedin.com/company/instalog-dev"
    }
  },
  "mcp": {
    "enabled": true,
    "title": "Instalog MCP",
    "description": "Enable AI assistants to interact with your Instalog data",
    "installation": "npx @mintlify/mcp add instalog"
  }
}